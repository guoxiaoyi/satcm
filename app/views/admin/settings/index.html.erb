<h1>Listing settings</h1>

<table>
  <thead>
    <tr>
      <th>Logo</th>
      <th>Tel</th>
      <th>Address</th>
      <th>Qrcode</th>
      <th>Banner a</th>
      <th>Banner b</th>
      <th>Oa path</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @settings.each do |setting| %>
      <tr>
        <td><%= setting.logo %></td>
        <td><%= setting.tel %></td>
        <td><%= setting.address %></td>
        <td><%= setting.qrcode %></td>
        <td><%= setting.banner_a %></td>
        <td><%= setting.banner_b %></td>
        <td><%= setting.oa_path %></td>
        <td><%= link_to 'Show', [:admin, setting] %></td>
        <td><%= link_to 'Edit', edit_admin_setting_path(setting) %></td>
        <td><%= link_to 'Destroy', [:admin, setting], method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Setting', new_admin_setting_path %>
