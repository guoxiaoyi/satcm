<%= render 'shared/admin/page_header', current_page: '导航列表', pages: @page_header %>
<div class="card">
  <div class="card-header">
    <h3 class="card-title">
      <%= link_to '后退', admin_organizations_path, class: "btn btn-default" %> |
      <%= link_to '编辑', edit_admin_organization_path(@organization), class: "btn btn-primary" %>
    </h3>
  </div>
  <!-- /.card-header -->
  <div class="card-body">
    <h3> <%= @organization.title %> </h3>
    <hr />
    <div class="content">
      <%= raw @organization.content %>
    </div>
  </div>
  <!-- /.card-body -->
</div>

